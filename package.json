{
  "name": "@strongishllama/populate-pull-request",
  "version": "0.1.0",
  "description": "Automatically populate pull requests based on predefined tags.",
  "main": "src/main.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strongishllama/pull-request-populate.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Taliesin Millhouse",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@octokit/openapi-types": "^11.2.0",
    "node-fetch": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@typescript-eslint/parser": "^5.11.0",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^26.1.0",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
